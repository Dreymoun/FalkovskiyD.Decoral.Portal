<!DOCTYPE html>
<html lang="en"
  data-wf-page="6332802bd593fd293c27cb70"
  data-wf-site="6332802bd593fd08af27cb6f"
>
  <head>
    <meta charset="utf-8">
    <title>Decoral - Webflow HTML website template</title>
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="Webflow" name="generator" />
    <link href="~/css/webflow-style.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com" rel="preconnect" />
    <link
      href="https://fonts.gstatic.com"
      rel="preconnect"
      crossorigin="anonymous"
    />
    <script
      src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"
      type="text/javascript"
    ></script>
    <script type="text/javascript">
      WebFont.load({
        google: {
          families: [
            "Oswald:200,300,400,500,600,700",
            "DM Serif Display:regular,italic",
            "Jost:regular,500,600,700",
          ],
        },
      });
    </script>
    <script type="text/javascript">
      !(function (o, c) {
        var n = c.documentElement,
          t = " w-mod-";
        (n.className += t + "js"),
          ("ontouchstart" in o ||
            (o.DocumentTouch && c instanceof DocumentTouch)) &&
            (n.className += t + "touch");
      })(window, document);
    </script>
    <link href="~/images/favicon.png" rel="shortcut icon" type="image/x-icon" />
    <link href="~/images/app-icon.png" rel="apple-touch-icon" />

    @RenderSection("styles", false)

  </head>
  <body>

    <main class="page-wrapper">
      <section class="header-section">

            @{
                Html.RenderPartial("Menu/_MainMenu");
            }

      </section>
      <section class="slider-section">
        <div class="slider-container">
          <div class="slider-container-inner">
            <div class="slider-content">
              <h1
                data-w-id="b6eadfa1-6e6a-63b6-d556-3637521cc677"
                style="opacity: 0"
                class="hero-heading"
              >
                Modern Intetior Design Studio.
              </h1>
              <a href="~/about" class="button-primary bg-color-black w-button"
                >Explore Now</a>
            </div>
            <div class="middle-image-block">
              <div class="image-wrapper animate-in">
                <div class="image-overlay"></div>
                <img src="~/images/slider-big-1.jpg"
                  loading="lazy"
                  srcset="~/images/slider-big-1-p-500.jpg 500w, images/slider-big-1.jpg       726w"
                  alt="Slider Image"
                  sizes="(max-width: 479px) 100vw, (max-width: 767px) 450px, (max-width: 991px) 480px, (max-width: 1279px) 50vw, (max-width: 1439px) 500px, (max-width: 1919px) 632px, 726px"
                  class="media-fit-image"
                />
              </div>
            </div>
            <div class="slider-another-content">
              <h3
                data-w-id="cadafd2e-18a8-c374-142a-f56d5c9a056b"
                style="opacity: 0"
                class="another-heading"
              >
                We will make the <span class="td-underline">Unique</span> &amp;
                <span class="td-underline">Modern</span> Style of your Interior
                design.
              </h3>
              <div class="another-image-block">
                <div class="image-wrapper animate-in">
                  <div class="image-overlay"></div>
                  <img src="~/images/slider-small-1.jpg"
                    loading="lazy"
                    alt="Slider Image"
                    class="media-fit-image"
                  />
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>

      @RenderBody()


        @{
            Html.RenderPartial("_Subscribe");
        }


        <footer class="footer-section">
            @{
                Html.RenderPartial("_Footer");
            }
        </footer>

        


    </main>
    <script src="~/js/jquery.js" type="text/javascript"></script>
    <script src="~/js/webflow-script.js" type="text/javascript"></script>

    @RenderSection("script", false)

</body>
</html>